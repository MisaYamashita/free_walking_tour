<div class="planned-past-wrapper"> <!--ユーザーが過去に開催したツアー一覧 -->
  <div class="planned-past-container container">
    <h3 class="text-center">開催したツアー一覧</h3>
    <div class="row">
      <div class="col-md-offset-3 col-md-6">
        <div class="tour-index-list">
          <% if @user_planned_past.exists? %>
            <% @user_planned_past.each do |tour| %>
              <div class="tour-box-list-past", data-turbolinks="false">
                <%= link_to tour.title, tour_path(id: tour.id) %><P>日時：<%= l tour.date %></P>
              </div>
            <% end %>
          <% else %>
            <h4>まだ<%= current_user.name %>さんが開催したツアーはありません</h4>
          <% end %>
          <%= will_paginate @user_planned_past, :previous_label => ' &lt 前へ', :next_label => '次へ &gt' %><br>
        </div>
      </div>
    </div>
  </div>
</div>