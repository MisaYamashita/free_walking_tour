<div class="joined-past-wrapper">　　<!--ユーザーが過去に参加したツアー一覧 -->
  <div class="joined-past-container container">
    <div class="row">
      <div class="col-md-offset-3 col-md-6">
        <h3>参加したツアー一覧</h3>
        <div class="tour-index-list">
          <% if @user_joined_past.exists? %>
            <% @user_joined_past.each do |tour| %>
              <div class="tour-box-list-past", data-turbolinks="false">
                <%= link_to tour.title, tour_path(id: tour.id) %><P>日時：<%= l tour.date %></P>
              </div>
            <% end %>
          <% else %>
            <h4>まだ<%= current_user.name %>さんが参加したツアーはありません</h4>
          <% end %>
          <%= will_paginate @user_joined_past, :previous_label => ' &lt 前へ', :next_label => '次へ &gt' %><br>
        </div>
      </div>
    </div>
  </div>
</div>