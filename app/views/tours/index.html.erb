<div class="tour-index-wrapper">
  <div class="tour-index-container">
    <h4>検索ツアー一覧</h4>
    <div class="tour-index-list">
      <% if @search_tours %>
        <% @search_tours.each do |tour| %>
          <%= link_to tour.title, '#' %><P>開催日時：<%= l tour.date %></P> <!--user_tour_path(user_id, @tour.id)?? -->
        <% end %>
      <% end %>
      <%= link_to '戻る', :back, class: "back-btn" %>
    </div>
  </div>
</div>