<div class="tour-index-wrapper">
  <div class="tour-index-container container">
    <h3 class="text-center">検索ツアー一覧</h3>
    <div class="row  justify-content-center">
      <div class="col-md-offset-3 col-md-6">
        <div class="tour-index-list">
          <% @search_tours.each do |tour| %>
            <div class="index-tour-box-list", data-turbolinks="false">
              <%= link_to tour.title, tour_path(id: tour.id) %><P>開催日時：<%= l tour.date %></P>
            </div>
          <% end %>
          <%= will_paginate @search_tours, :previous_label => ' &lt 前へ', :next_label => '次へ &gt' %><br>
        </div>
      </div>
    </div>
  </div>
</div>